{
  "name": "are-we-flying",
  "version": "1.0.0",
  "description": "Flying planes with JavaScript",
  "type": "module",
  "main": "README.md",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "start": "run-p api web",
    "mock": "run-p web api:mock",
