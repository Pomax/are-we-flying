    "canvas": "^2.11.2",
    "cors": "^2.8.5",
    "crc": "^4.3.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-http-proxy": "^1.6.3",
    "express-ws": "^5.0.2",
    "geomag": "^1.0.0",
    "helmet": "^6.0.1",
    "msfs-simconnect-api-wrapper": "^2.0.4",
